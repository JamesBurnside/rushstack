{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "./testfile.d.ts",
  "apiReport": {
    "enabled": true,
    "reportFolder": "./test-review/"
  },
  "docModel": {
    "enabled": true
  },
  "dtsRollup": {
    "enabled": true
  },
  "messages": {
    "extractorMessageReporting": {
      "ae-missing-release-tag": {
        "logLevel": "none"
      },
      "ae-unresolved-link": {
        "logLevel": "none"
      }
    }
  }
}
